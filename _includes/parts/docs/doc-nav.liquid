<nav class="w-full md:w-56 mb-5">
    <headless-disclosure
            class="sticky block top-32 space-y-5 bg-white/10 dark:bg-gray-900/10 border border-gray-200 dark:border-gray-900 backdrop-blur rounded-md p-5">
        {% for items in site.data.docnav %}
            <button type="button" aria-controls="{{items.name}}" aria-expanded="true" class="text-xl font-semibold w-full flex ">{{items.name}}
                <span class="ml-2 translate-y-1">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-5 h-5"><path fill-rule="evenodd" d="M10 3a.75.75 0 01.55.24l3.25 3.5a.75.75 0 11-1.1 1.02L10 4.852 7.3 7.76a.75.75 0 01-1.1-1.02l3.25-3.5A.75.75 0 0110 3zm-3.76 9.2a.75.75 0 011.06.04l2.7 2.908 2.7-2.908a.75.75 0 111.1 1.02l-3.25 3.5a.75.75 0 01-1.1 0l-3.25-3.5a.75.75 0 01.04-1.06z" clip-rule="evenodd"/></svg>
                </span>
            </button>
            <div id="{{items.name}}" class=" space-y-5">
                {% for item in items.sub %}
                    <a class="flex items-center" href="{{ item.url | relative_url }}">
                        {{ item.name}}
                    </a>
                {% endfor %}
            </div>
        {% endfor %}
    </headless-disclosure>
</nav>